{
  "name": "backend",
  "type": "commonjs",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "start": "node dist/server.js",
    "build": "pnpm install --no-frozen-lockfile && pnpm prisma:generate && tsc",
    "deploy": "pnpm install --prod --no-frozen-lockfile && pnpm prisma:generate && pnpm build",
    "prisma:generate": "prisma generate && prisma generate --schema=prisma-mongo/schema.prisma",
    "prisma:pull": "prisma db pull && prisma generate && prisma db pull --schema=prisma-mongo/schema.prisma && prisma generate --schema=prisma-mongo/schema.prisma",
    "seed": "ts-node-dev prisma/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.919.0",
    "@aws-sdk/s3-request-presigner": "^3.919.0",
    "@prisma/client": "^6.18.0",
    "@types/node-cron": "^3.0.11",
    "@zalopay-oss/zalopay-nodejs": "^0.4.0",
    "axios": "^1.13.1",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "dotenv": "^17.2.2",
    "exceljs": "^4.4.0",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "inversify": "^7.10.4",
    "jsonwebtoken": "^9.0.2",
    "mime-types": "^3.0.1",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.9",
    "pino": "^9.9.5",
    "prisma": "^6.18.0",
    "puppeteer": "^24.27.0",
    "recharts": "^3.3.0",
    "reflect-metadata": "^0.2.2",
    "uuid": "^13.0.0",
    "vnpay": "^2.4.4",
    "xlsx": "^0.18.5",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/helmet": "^4.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mime-types": "^3.0.1",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.3.3",
    "@types/nodemailer": "^7.0.2",
    "@types/uuid": "^11.0.0",
    "@types/xlsx": "^0.0.36",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  },
  "prisma": {
    "schema": "prisma/schema.prisma"
  }
}
